            collision = collisionPlane.findCollisionQuadratic(p.position, p.velocity, force, collisionEpsilon, forceEpsilon)
            if (collision is not None):
                #print "collideWithPlane position {} velocity {} force {} time {} result {}".format(p.position, p.velocity, force, collision.time, collision.point)
            return collision


            print "freeAdvance position {} velocity {} force {} time {}".format(p.position, p.velocity, force, timeToGo)
            print "result {}".format(p.position)
